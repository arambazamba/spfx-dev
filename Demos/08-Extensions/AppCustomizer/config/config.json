{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "placeholder-sample-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/placeholderSample/PlaceholderSampleApplicationCustomizer.js",
          "manifest": "./src/extensions/placeholderSample/PlaceholderSampleApplicationCustomizer.manifest.json"
        }
      ]
    },
    "toast-customizer-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/toastCustomizer/ToastCustomizerApplicationCustomizer.js",
          "manifest": "./src/extensions/toastCustomizer/ToastCustomizerApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://code.jquery.com/jquery-2.1.1.min.js",
      "globalName": "jquery"
    },
    "toastr": "https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js",
    "sp-pnp-js": "node_modules/sp-pnp-js/dist/pnp.min.js"
  },
  "localizedResources": {
    "PlaceholderSampleApplicationCustomizerStrings": "lib/extensions/placeholderSample/loc/{locale}.js",
    "ToastCustomizerApplicationCustomizerStrings": "lib/extensions/toastCustomizer/loc/{locale}.js"
  }
}
